<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.qhy.test.gencodeDAM">
	<class
		name="DamApplicationMetadata"
		table="DAM_APPLICATION_METADATA"
	>
		<meta attribute="sync-DAO">true</meta>

		<property
			name="ApplicationId"
			column="APPLICATION_ID"
			type="string"
			not-null="false"
			length="32"
		/>
		<property
			name="MetaDataId"
			column="META_DATA_ID"
			type="string"
			not-null="false"
			length="32"
		/>


	</class>	
</hibernate-mapping>