<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.qhy.test.gencodeDAM">
	<class
		name="DamApplicationInfo"
		table="DAM_APPLICATION_INFO"
	>
		<meta attribute="sync-DAO">true</meta>

		<property
			name="ApplicationId"
			column="APPLICATION_ID"
			type="string"
			not-null="false"
			length="32"
		/>
		<property
			name="ApplicationName"
			column="APPLICATION_NAME"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="ApplicationNo"
			column="APPLICATION_NO"
			type="string"
			not-null="false"
			length="32"
		/>
		<property
			name="ApplicationOwner"
			column="APPLICATION_OWNER"
			type="string"
			not-null="false"
			length="32"
		/>
		<property
			name="LogicLayer"
			column="LOGIC_LAYER"
			type="integer"
			not-null="false"
			length="22"
		/>
		<property
			name="ServerId"
			column="SERVER_ID"
			type="string"
			not-null="false"
			length="32"
		/>
		<property
			name="ApplicationSituation"
			column="APPLICATION_SITUATION"
			type="string"
			not-null="false"
			length="500"
		/>
		<property
			name="ApplicationType"
			column="APPLICATION_TYPE"
			type="integer"
			not-null="false"
			length="22"
		/>
		<property
			name="ResultType"
			column="RESULT_TYPE"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="ModelType"
			column="MODEL_TYPE"
			type="integer"
			not-null="false"
			length="22"
		/>
		<property
			name="ApplicationScore"
			column="APPLICATION_SCORE"
			type="big_decimal"
			not-null="false"
			length="4"
		/>
		<property
			name="EvaluateLevel"
			column="EVALUATE_LEVEL"
			type="integer"
			not-null="false"
			length="22"
		/>
		<property
			name="CreateTime"
			column="CREATE_TIME"
			type="string"
			not-null="false"
			length="4"
		/>
		<property
			name="RecoveryModel"
			column="RECOVERY_MODEL"
			type="integer"
			not-null="false"
			length="22"
		/>
		<property
			name="Status"
			column="STATUS"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="DescTxt"
			column="DESC_TXT"
			type="string"
			not-null="false"
			length="500"
		/>
		<property
			name="BusinessValue"
			column="BUSINESS_VALUE"
			type="string"
			not-null="false"
			length="500"
		/>
		<property
			name="DataSource"
			column="DATA_SOURCE"
			type="string"
			not-null="false"
			length="500"
		/>


	</class>	
</hibernate-mapping>